<p id="notice"><%= notice %></p>

<h1>Class Lists</h1>

<table>
  <thead>
    <tr>
      <th>C name</th>
      <th>Credit</th>
      <th>Classify</th>
      <th>Sub type</th>
      <th>Sub num</th>
      <th>Target grade</th>
      <th>Limit num</th>
      <th>Lecture time</th>
      <th>Department</th>
      <th>Professor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @class_lists.each do |class_list| %>
      <tr>
        <td><%= class_list.c_name %></td>
        <td><%= class_list.credit %></td>
        <td><%= class_list.classify %></td>
        <td><%= class_list.sub_type %></td>
        <td><%= class_list.sub_num %></td>
        <td><%= class_list.target_grade %></td>
        <td><%= class_list.limit_num %></td>
        <td><%= class_list.lecture_time %></td>
        <td><%= class_list.department %></td>
        <td><%= class_list.professor %></td>
        <td><%= link_to 'Show', class_list %></td>
        <td><%= link_to 'Edit', edit_class_list_path(class_list) %></td>
        <td><%= link_to 'Destroy', class_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Class List', new_class_list_path %>
